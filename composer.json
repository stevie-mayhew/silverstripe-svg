{
    "name": "stevie-mayhew/silverstripe-svg",
    "type": "silverstripe-vendormodule",
    "description": "Basic SVG support for SilverStripe",
    "keywords": ["silverstripe", "svg", "template"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stevie Mayhew",
            "email": "stevie@stevie.co.nz"
        }
    ],
    "require": {
        "silverstripe/framework": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "StevieMayhew\\SilverStripeSVG\\": "src/",
            "StevieMayhew\\SilverStripeSVG\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
